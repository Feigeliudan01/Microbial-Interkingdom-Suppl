##draw figure
library(ggplot2)

p<- ggplot(Figure5B_inputdata, aes(x=Condition, y=Normalized_to_bacteria))+
	geom_boxplot(alpha=.2, size=1.5)+
	geom_jitter(aes(height=0, size=4, fill="white"))+
	theme(
	panel.background = element_rect(fill = "transparent",colour = NA),
    plot.background = element_rect(fill = "transparent",colour = NA)
   )
##calculate significant differences

library(agricolae)

kruskal(Figure5B_inputdata$Normalized_to_bacteria, Figure5B_inputdata$Condition, alpha=0.05, p.adj="bonferroni")$groups
